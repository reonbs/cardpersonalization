<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="Coderthemes">

    <!-- App favicon -->
    <link rel="shortcut icon" href="@Url.Content("~/assets/images/zenith-bank-logo_1.ico")">
    <!-- App title -->
    <title>PictureCard</title>

    <!-- Custom box css -->
    <link href="@Url.Content("~/plugins/custombox/css/custombox.min.css")" rel="stylesheet">

    <!-- Notification css (Toastr) -->
    <link href="@Url.Content("~/plugins/toastr/toastr.min.css")" rel="stylesheet" type="text/css" />

    <!-- DataTables -->
    <link href="@Url.Content("~/plugins/datatables/jquery.dataTables.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/plugins/datatables/buttons.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/plugins/datatables/fixedHeader.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/plugins/datatables/responsive.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/plugins/datatables/scroller.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/plugins/datatables/dataTables.colVis.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/plugins/datatables/dataTables.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/plugins/datatables/fixedColumns.dataTables.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/plugins/jQuery-ui/jQuery-ui.min.css")" rel="stylesheet">

    <!-- App css -->
    <link href="@Url.Content("~/assets/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/core.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/components.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/icons.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/pages.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/menu.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/responsive.css")" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/plugins/switchery/switchery.min.css")">

    <!--Loading-->
    <link href="@Url.Content("~/Scripts/Loading/waitMe.min.css")" rel="stylesheet" />

    <!-- HTML5 Shiv and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

    <script src="@Url.Content("~/assets/js/modernizr.min.js")"></script>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
</head>
<body class="fixed-left">
    <div id="wrapper">
        <!-- Top Bar Start -->
        <div class="topbar">

            <!-- LOGO -->
            <div class="topbar-left">
                @*<a href="index.html" class="logo">
                        <p>Cards</p>

                        <i class="mdi mdi-layers"></i>
                    </a>*@
                <!-- Image logo -->
                <a href="index.html" class="logo">
                    <span>
                        <img src="~/images/zenith-bank-logo_1.png" alt="" height="70">

                    </span>
                    <i>
                        <img src="~/images/zenith-bank-logo_1.png" alt="" height="28">
                    </i>
                </a>
            </div>

            <!-- Button mobile view to collapse sidebar menu -->
            <div id="topNavBar" class="navbar navbar-default" role="navigation" style="background:#e2000f3;">
                <div class="container">

                    <!-- Navbar-left -->
                    <ul class="nav navbar-nav navbar-left">
                        <li>
                            <button class="button-menu-mobile open-left waves-effect">
                                <i class="mdi mdi-menu"></i>
                            </button>
                        </li>
                        <li class="hidden-xs">
                            @*<form role="search" class="app-search">
                                    <input type="text" placeholder="Search..." class="form-control">
                                    <a href="">
                                        <i class="fa fa-search"></i>
                                    </a>
                                </form>*@
                            <br />
                            <span style="margin:19px 12px 0 5px;">Card Personalisation</span>
                        </li>
                    </ul>

                    <!-- Right(Notification) -->
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            @Html.Partial("_LoginPartial")
                        </li>
                    </ul>
                    @*<div class="row">
                            <div class="col-md-12">
                                <div id="loginStatus" class="pull-right">

                                    @Html.Partial("_LoginPartial")

                                </div>
                            </div>
                        </div>*@

                    <!-- end navbar-right -->

                </div>
                <!-- end container -->
            </div>
            <!-- end navbar -->
        </div>
        <!-- Top Bar End -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left side-menu">
            <div class="sidebar-inner slimscrollleft">

                <!--- Sidemenu -->
                @Html.Partial("_SideMenuPartial")
                <!-- Sidebar -->
                <div class="clearfix"></div>

            </div>
            <!-- Sidebar -left -->

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container">

                    <br />
                    <!-- end row -->


                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card-box">


                                @RenderBody()


                            </div>
                        </div>
                    </div>
                    <!-- end row -->


                </div>
                <!-- container -->

            </div>
            <!-- content -->

            <footer class="footer text-right">
                <p>&copy; @DateTime.Now.Year - Card Personalization</p>
            </footer>

        </div>


        <!-- ============================================================== -->
        <!-- End Right content here -->
        <!-- ============================================================== -->
        <!-- Right Sidebar -->
        <div class="side-bar right-bar">
            <a href="javascript:void(0);" class="right-bar-toggle">
                <i class="mdi mdi-close-circle-outline"></i>
            </a>
            <h4 class="">Settings</h4>
            <div class="setting-list nicescroll">
                <div class="row m-t-20">
                    <div class="col-xs-8">
                        <h5 class="m-0">Notifications</h5>
                        <p class="text-muted m-b-0">
                            <small>Do you need them?</small>
                        </p>
                    </div>
                    <div class="col-xs-4 text-right">
                        <input type="checkbox" checked data-plugin="switchery" data-color="#7fc1fc" data-size="small" />
                    </div>
                </div>

                <div class="row m-t-20">
                    <div class="col-xs-8">
                        <h5 class="m-0">API Access</h5>
                        <p class="m-b-0 text-muted">
                            <small>Enable/Disable access</small>
                        </p>
                    </div>
                    <div class="col-xs-4 text-right">
                        <input type="checkbox" checked data-plugin="switchery" data-color="#7fc1fc" data-size="small" />
                    </div>
                </div>

                <div class="row m-t-20">
                    <div class="col-xs-8">
                        <h5 class="m-0">Auto Updates</h5>
                        <p class="m-b-0 text-muted">
                            <small>Keep up to date</small>
                        </p>
                    </div>
                    <div class="col-xs-4 text-right">
                        <input type="checkbox" checked data-plugin="switchery" data-color="#7fc1fc" data-size="small" />
                    </div>
                </div>

                <div class="row m-t-20">
                    <div class="col-xs-8">
                        <h5 class="m-0">Online Status</h5>
                        <p class="m-b-0 text-muted">
                            <small>Show your status to all</small>
                        </p>
                    </div>
                    <div class="col-xs-4 text-right">
                        <input type="checkbox" checked data-plugin="switchery" data-color="#7fc1fc" data-size="small" />
                    </div>
                </div>
            </div>
        </div>
        <!-- /Right-bar -->
    </div>

    <script>
        var resizefunc = [];
    </script>

    <!-- jQuery  -->
    <script src="@Url.Content("~/assets/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/assets/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/assets/js/detect.js")"></script>
    <script src="@Url.Content("~/assets/js/fastclick.js")"></script>
    <script src="@Url.Content("~/assets/js/jquery.blockUI.js")"></script>
    <script src="@Url.Content("~/assets/js/waves.js")"></script>
    <script src="@Url.Content("~/assets/js/jquery.slimscroll.js")"></script>
    <script src="@Url.Content("~/assets/js/jquery.scrollTo.min.js")"></script>
    <script src="@Url.Content("~/plugins/switchery/switchery.min.js")"></script>

    <!--DataTables-->
    <script src="@Url.Content("~/plugins/datatables/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/dataTables.bootstrap.js")"></script>

    <script src="@Url.Content("~/plugins/datatables/dataTables.buttons.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/buttons.bootstrap.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/jszip.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/pdfmake.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/vfs_fonts.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/buttons.html5.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/buttons.print.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/dataTables.fixedHeader.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/dataTables.keyTable.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/dataTables.responsive.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/responsive.bootstrap.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/dataTables.scroller.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/dataTables.colVis.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/dataTables.fixedColumns.min.js")"></script>

    <!-- init -->
    <script src="@Url.Content("~/assets/pages/jquery.datatables.init.js")"></script>
    <script src="@Url.Content("~/plugins/jQuery-ui/jquery-ui.min.js")"></script>
   
    <!-- Toastr js -->
    <script src="@Url.Content("~/plugins/toastr/toastr.min.js")"></script>
    <!-- Toastr init js (Demo)-->
    <script src="@Url.Content("~/assets/pages/jquery.toastr.js")"></script>

    <!-- Modal-Effect -->
    <script src="@Url.Content("~/plugins/custombox/js/custombox.min.js")"></script>
    <script src="@Url.Content("~/plugins/custombox/js/legacy.min.js")"></script>

   
    <script src="@Url.Content("~/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js")" type="text/javascript"></script>

    <script type="text/javascript" src="@Url.Content("~/plugins/autocomplete/jquery.mockjax.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/plugins/autocomplete/jquery.autocomplete.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/plugins/autocomplete/countries.js")"></script>
    @*<script type="text/javascript" src="@Url.Content("~/assets/pages/jquery.autocomplete.init.js"></script>*@

    <!-- App js -->
    <script src="@Url.Content("~/assets/js/jquery.core.js")"></script>
    <script src="@Url.Content("~/assets/js/jquery.app.js")"></script>

    <!--Loading-->
    <script src="~/Scripts/Loading/waitMe.min.js"></script>
    <script src="~/Scripts/Loading/myLoadingOption.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
